/* 0: normal, 1: bottom up, 2: left up, 3: right up */
orientation = <0>;

/* reset, power */
reset-gpios = <&gpa7 1 GPIO_ACTIVE_HIGH>;
vddi-supply = <&m_ldo24_reg>;
vci-supply = <&disp_vci>;
